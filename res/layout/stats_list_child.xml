<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#dddddd"
     >

    <LinearLayout
        android:id="@+id/stats_list_child_linearLayout1"
        android:layout_width="fill_parent"
        android:layout_height="2dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:background="#aaafff"
        android:orientation="vertical" >
    </LinearLayout>
    <LinearLayout
        android:id="@+id/stats_list_child_linearLayout4"
        android:layout_width="fill_parent"
        android:layout_height="2dp"
        android:layout_alignParentLeft="true"
        android:layout_below="@+id/stats_list_child_linearLayout1"
        android:layout_marginTop="3dp"
        android:background="#aaafff"
        android:orientation="vertical" >
    </LinearLayout>

    <TextView
        android:id="@+id/stats_list_child_textView1"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_below="@+id/stats_list_child_linearLayout4"
        android:layout_marginTop="9dp"
        android:gravity="center"
        android:text="4 oose prac are left"
        android:textAppearance="?android:attr/textAppearanceMedium" />

    <TextView
        android:id="@+id/stats_list_child_textView2"
        android:layout_marginLeft="10dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_below="@+id/stats_list_child_textView1"
        android:layout_marginTop="20dp"
        android:text="If you bunk 2"
        android:textAppearance="?android:attr/textAppearanceSmall" />

    <TextView
        android:id="@+id/stats_list_child_textView3"
        android:layout_marginRight="10dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/stats_list_child_textView2"
        android:layout_alignBottom="@+id/stats_list_child_textView2"
        android:layout_alignParentRight="true"
        android:text="75%"
        android:textAppearance="?android:attr/textAppearanceSmall" />

    <TextView
        android:id="@+id/stats_list_child_textView4"
        android:layout_marginLeft="10dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_below="@+id/stats_list_child_textView2"
        android:layout_marginTop="5dp"
        android:text="If you attend all 4"
        android:textAppearance="?android:attr/textAppearanceSmall" />

    <TextView
        android:id="@+id/stats_list_child_textView5"
        android:layout_width="wrap_content"
        android:layout_marginRight="10dp"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/stats_list_child_textView4"
        android:layout_alignBottom="@+id/stats_list_child_textView4"
        android:layout_alignParentRight="true"
        android:text="81%"
        android:textAppearance="?android:attr/textAppearanceSmall" />

    <TextView
        android:id="@+id/stats_list_child_textView6"
        android:layout_width="wrap_content"
        android:layout_marginLeft="10dp"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_below="@+id/stats_list_child_textView4"
        android:layout_marginTop="5dp"
        android:text="If you attend none"
        android:textAppearance="?android:attr/textAppearanceSmall" />

    <TextView
        android:id="@+id/stats_list_child_textView7"
        android:layout_width="wrap_content"
        android:layout_marginRight="10dp"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/stats_list_child_textView6"
        android:layout_alignBottom="@+id/stats_list_child_textView6"
        android:layout_alignParentRight="true"
        android:text="40%"
        android:textAppearance="?android:attr/textAppearanceSmall" />

    <TextView
        android:id="@+id/stats_list_child_textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/stats_list_child_textView6"
        android:layout_below="@+id/stats_list_child_textView6"
        android:layout_marginTop="5dp"
        android:text="If you bunk n"
        android:textAppearance="?android:attr/textAppearanceMedium" />

    <TextView
        android:id="@+id/stats_list_child_textView9"
        android:layout_width="wrap_content"
        android:layout_marginRight="10dp"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/stats_list_child_textView8"
        android:layout_alignBottom="@+id/stats_list_child_textView8"
        android:layout_alignParentRight="true"
        android:text="-"
        android:textAppearance="?android:attr/textAppearanceMedium" />

    <RelativeLayout
        android:id="@+id/stats_list_child_seekBarContainer"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/stats_list_child_textView8"
        android:layout_below="@+id/stats_list_child_textView8"
        
        
         >
    <SeekBar
        android:id="@+id/stats_list_child_seekBar1"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_centerVertical="true"
        android:layout_marginTop="20dp" 
        android:focusable="false"/>
	</RelativeLayout>

    <LinearLayout
        android:id="@+id/stats_list_child_linearLayout2"
        android:layout_width="fill_parent"
        android:layout_height="2dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentRight="true"
        android:layout_below="@+id/stats_list_child_seekBarContainer"
        android:layout_marginTop="5dp"
        android:background="#aaafff"
        android:orientation="vertical" >

    </LinearLayout>

    <LinearLayout
        android:id="@+id/stats_list_child_linearLayout3"
        android:layout_width="fill_parent"
        android:layout_height="2dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentRight="true"
        android:layout_below="@+id/stats_list_child_linearLayout2"
        android:layout_marginTop="3dp"
        android:background="#aaafff"
        android:orientation="vertical" >

    </LinearLayout>

</RelativeLayout>